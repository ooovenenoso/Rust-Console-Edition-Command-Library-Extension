{
  "manifest_version": 3,
  "name": "Rust Console Edition: Command Library",
  "version": "1.0",
  "description": "A helper extension for Rust Console Edition commands.",
  "icons": {
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon128.png"
    }
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
